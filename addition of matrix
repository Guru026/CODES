#include <stdio.h>
#include <conio.h>

int main()
{
    int a[5][5],b[5][5],i,j,m,n;
    int c[5][5];
    printf("enter the order of your matrixes");
    scanf("%d %d",&m,&n);
    printf("enter elements of your matrix A");
    for(i=0 ; i<m ; i++)
    {
        for(j=0 ; j<n ; j++)
        {
        scanf("%d",&a[i][j]);
        }
    }
    printf("enter elements of your matrix B");
    for(i=0 ; i<m ; i++)
    {
        {
        for(j=0 ; j<n ; j++)
        scanf("%d",&b[i][j]);
        }
    }
        c[i][j]=0;
        c[i][j] = a[i][j] + b[i][j];
    for(i=0 ; i<m ; i++)
    {
        {
        for(j=0 ; j<n ; j++)
        printf("%d\t",c[i][j]);
        }
    printf("\n");
    }
    return 0;
}
